#include <iostream.h>
int main()
{
int n;
long long factorial = 1;
cout << "Enter a positive integer: ";
cin >> n;
for(int i = 1; i <=n; ++i){
factorial *= i;
}
cout << "Factorial of " << n << " = " << factorial;    return 0;
}








OUTPUT
Enter a positive integer: 12
Factorial of 12 = 479001600
