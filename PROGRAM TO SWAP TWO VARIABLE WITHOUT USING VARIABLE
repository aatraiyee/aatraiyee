#include <stdio.h>
int main()
{
int x, y, t;
printf ("Enter two integers\n");
scanf ("%d%d", &x, &y);
printf ("Before Swapping\n First integer = %d\n Second integer = %d\n", x, y);
t = x;
x = y;
y = t;
printf ("After Swapping\n First integer = %d\n Second integer = %d\n", x, y);
return 0;
}









OUTPUT
Enter two integers
23
45
Before Swapping
First integer = 23
Second integer = 45
After Swapping
First integer = 45
Second integer = 23
